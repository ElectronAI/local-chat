{
  "name": "LocalChat",
  "version": "0.4.0",
  "description": "LocalChat is your personal assistant: An LLM running locally on your computer.",
  "main": ".webpack/main",
  "author": "Hendrik Erz <hendrik@zettlr.com>",
  "license": "GPL-3.0-only",
  "private": false,
  "scripts": {
    "prepare-release": "./scripts/prepare-release.sh",
    "start": "NODE_ENV=develop electron-forge start",
    "package": "electron-forge package",
    "package:mac-x64": "electron-forge package --platform=darwin --arch=x64",
    "package:mac-arm": "electron-forge package --platform=darwin --arch=arm64",
    "package:win-x64": "electron-forge package --platform=win32 --arch=x64",
    "package:linux-x64": "electron-forge package --platform=linux --arch=x64",
    "package:linux-arm": "electron-forge package --platform=linux --arch=arm64",
    "release:mac-x64": "electron-forge make --platform=darwin --arch=x64",
    "release:mac-arm": "electron-forge make --platform=darwin --arch=arm64",
    "release:win-x64": "electron-forge make --platform=win32 --arch=x64",
    "release:linux-x64": "electron-forge make --platform=linux --arch=x64",
    "release:linux-arm": "electron-forge make --platform=linux --arch=arm64"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@types/luxon": "^3.4.0",
    "@types/showdown": "^2.0.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.9.0",
    "electron": "^28.1.3",
    "electron-devtools-assembler": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-vue": "^9.20.0",
    "feather-icons": "^4.29.1",
    "gguf": "^1.0.3",
    "got": "11.x.x",
    "highlight.js": "^11.9.0",
    "highlightjs-copy": "^1.0.5",
    "luxon": "^3.4.4",
    "node-llama-cpp": "^3.0.0-beta.5",
    "node-loader": "^2.0.0",
    "pinia": "^2.1.7",
    "showdown": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.10",
    "vue-feather": "^2.0.0",
    "vue-loader": "^17.4.2"
  },
  "dependencies": {}
}
